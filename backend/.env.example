# AI Studio - Environment Configuration
# Copy this file to .env and adjust settings as needed

# ============================================
# Content Filtering
# ============================================

# NSFW (Not Safe For Work) Content Filter
# true  = Filter disabled (allow all content) - DEFAULT
# false = Filter enabled (block NSFW content, returns black images)
DISABLE_NSFW_FILTER=true

# ============================================
# GPU Settings
# ============================================

# Device selection
# cuda = NVIDIA GPU (default)
# cpu  = CPU only (slower)
# mps  = Apple Silicon GPU
DEVICE=cuda

# Memory Optimizations (for CUDA)
ENABLE_XFORMERS=true
ENABLE_ATTENTION_SLICING=true
VAE_SLICING=true

# ============================================
# API Settings
# ============================================

# API Server Configuration
API_HOST=127.0.0.1
API_PORT=8000

# Debug Mode
DEBUG=true

# ============================================
# Generation Defaults
# ============================================

DEFAULT_WIDTH=512
DEFAULT_HEIGHT=512
DEFAULT_STEPS=30
DEFAULT_GUIDANCE=7.5
MAX_BATCH_SIZE=4

# ============================================
# Paths (Optional - uses defaults if not set)
# ============================================

# MODELS_DIR=./models
# OUTPUTS_DIR=./outputs
# DB_PATH=./ai_studio.db
